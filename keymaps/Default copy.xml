<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateAndroidMonitorToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateTODOToolWindow" />
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="SearchEverywhere">
    <keyboard-shortcut first-keystroke="shift space" />
  </action>
  <action id="StepInto">
    <keyboard-shortcut first-keystroke="f7" />
    <mouse-shortcut keystroke="button5" />
  </action>
  <action id="StepOver">
    <keyboard-shortcut first-keystroke="f8" />
    <mouse-shortcut keystroke="button4" />
  </action>
  <action id="SurroundWith">
    <keyboard-shortcut first-keystroke="ctrl alt t" />
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
</keymap>